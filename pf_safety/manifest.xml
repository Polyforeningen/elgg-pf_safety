<?xml version="1.0" encoding="UTF-8"?>
<plugin_manifest xmlns="http://www.elgg.org/plugin_manifest/1.8">
  <name>Polyf√∂reningen safety hacks</name>
  <author>Richard Levitte</author>
  <version>0.9</version>
  <license>Apache Licensen v2.0</license>
  <description>We want to make some pages available for logged
  in users only.  This plugin is a collection of hacks doing
  just that.

  The currently affected plugins are:
  * members
  </description>
 
  <requires>
    <type>elgg_release</type>
    <version>1.8</version>
  </requires>

  <requires>
    <type>plugin</type>
    <name>members</name>
    <version>1.8</version>
    <comparison>eq</comparison>
  </requires>
  <requires>
    <type>priority</type>
    <plugin>members</plugin>
    <priority>after</priority>
  </requires>

  <requires>
    <type>plugin</type>
    <name>groups</name>
    <version>1.8</version>
    <comparison>eq</comparison>
  </requires>
  <requires>
    <type>priority</type>
    <plugin>groups</plugin>
    <priority>after</priority>
  </requires>

  <requires>
    <type>plugin</type>
    <name>profile</name>
    <version>1.8</version>
    <comparison>eq</comparison>
  </requires>
  <requires>
    <type>priority</type>
    <plugin>profile</plugin>
    <priority>after</priority>
  </requires>

</plugin_manifest>
